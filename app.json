{
  "id": "emometer",
  "cpus": 0.25,
  "mem": 512.0,
  "cmd": "npm start",
  "env": {
    "BAMBOO_SUBDOMAIN": "emometer"
  },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "docker.nrk.no:5000/nrknoutvikling/emometer/85cebac",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8081,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ]
    },
    "volumes": []
  },
  "instances": 1,
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/",
      "gracePeriodSeconds": 5,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}
